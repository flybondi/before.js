{
  "name": "@flybondi/before.js",
  "version": "0.0.0-development",
  "description": "Enables data fetching with any React SSR app that uses React Router 4",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "author": "Flybondi <dev@flybondi.com>",
  "license": "UNLICENSED",
  "keywords": [
    "react",
    "react ssr",
    "react-router",
    "before",
    "async"
  ],
  "scripts": {
    "build.code": "cross-env NODE_ENV=production rollup -c",
    "build": "run-s build.*",
    "flow.coverage": "flow coverage",
    "flow.install": "flow-typed install --ignoreDeps dev",
    "flow.start": "flow start",
    "flow.status": "flow status",
    "flow.stop": "flow stop",
    "lint": "eslint src/",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --write src/",
    "start": "",
    "test.coverage": "yarn test --coverage --coverageDirectory .coverage",
    "test.watch": "jest --watchAll",
    "test": "jest"
  },
  "peerDependencies": {
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "react": ">=16",
    "react-dom": ">= 16",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.2.1",
    "query-string": "^6.2.0",
    "ramda": "^0.26.1",
    "serialize-javascript": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@flybondi/rollup-plugin-ramda": "^1.0.0",
    "@loadable/component": "^5.2.2",
    "@loadable/server": "^5.2.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-react-app": "^7.0.0",
    "commitlint": "^7.2.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "0.89.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "object-assign": "^4.1.1",
    "prettier": "^1.15.3",
    "raf": "^3.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "rollup": "^1.1.0",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-bundle-size": "^1.0.2",
    "rollup-plugin-clean": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-progress": "^1.0.0",
    "rollup-plugin-visualizer": "^0.9.2",
    "run-sequence": "^2.2.1",
    "webpack": "^4.28.4",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "run-s test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/enzyme.config.js",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "testURL": "http://localhost"
  }
}
